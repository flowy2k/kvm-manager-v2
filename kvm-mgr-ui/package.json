{
  "name": "kvm-manager-app",
  "version": "1.0.0",
  "description": "Complete KVM Manager Application - Node.js Express server with Python backend integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "echo 'Building production assets...' && echo 'Build complete'",
    "python:start": "cd ../kvm-mgr-service && uv run python main.py",
    "python:install": "cd ../kvm-mgr-service && uv sync",
    "frontend": "python3 -m http.server 8081",
    "docker:build": "cd .. && docker build -t kvm-manager .",
    "docker:run": "cd .. && ./deploy.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "kvm",
    "manager",
    "express",
    "python",
    "serial",
    "hardware"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
